{
    "agent": {
      "metrics_collection_interval": 60,
      "run_as_user": "cwagent"
    },
    "metrics": {
      "namespace": "YoctoMetrics",
      "metrics_collected": {
        "cpu": {
          "resources": [
            "*"
          ],
          "measurement": [
            "usage_active",
            "usage_idle",
            "usage_system",
            "usage_user"
          ],
          "totalcpu": false
        },
        "mem": {
          "measurement": [
            "used_percent",
            "used",
            "total"
          ]
        },
        "disk": {
          "resources": [
            "/"
          ],
          "measurement": [
            "used_percent",
            "used",
            "total"
          ]
        },
        "diskio": {
          "resources": [
            "*"
          ],
          "measurement": [
            "reads",
            "writes",
            "read_bytes",
            "write_bytes",
            "io_time"
          ]
        },
        "netstat": {
          "measurement": [
            "tcp_established",
            "tcp_time_wait"
          ]
        }
      }
    }
  }
  